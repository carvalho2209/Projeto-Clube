<Window x:Class="WAssociado.frmAssociado"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Consulta Associados" Height="338" Width="616" Name="Associado">
    <Grid>
        <TextBox Height="28" HorizontalAlignment="Right" Margin="0,37,294,0" Name="txtIdentificador" VerticalAlignment="Top" Width="120" />
        <Label Content="Identificador" Height="28" HorizontalAlignment="Left" Margin="84,37,0,0" Name="label1" VerticalAlignment="Top" />
        <TextBox Height="28" HorizontalAlignment="Left" Margin="180,116,0,0" Name="txtEndereco" VerticalAlignment="Top" Width="271" />

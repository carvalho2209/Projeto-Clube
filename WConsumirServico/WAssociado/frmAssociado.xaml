        <Button Content="Consultar Sync" Height="27" HorizontalAlignment="Left" Margin="306,38,0,0" Name="btnConsultar" VerticalAlignment="Top" Width="95" Click="btnConsultar_Click" />
        <Button Content="Consultar Assync" Height="28" HorizontalAlignment="Right" Margin="0,37,73,0" Name="btnConsultar2" VerticalAlignment="Top" Width="114" Click="btnConsultar2_Click" />
